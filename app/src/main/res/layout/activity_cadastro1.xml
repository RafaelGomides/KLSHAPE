<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:app="http://schemas.android.com/apk/res-auto"
xmlns:tools="http://schemas.android.com/tools"
android:layout_width="match_parent"
android:layout_height="match_parent"
android:background="@drawable/fundo"
tools:context="com.example.pichau.projeto_tcc.Cadastro1">


<ImageView
    android:id="@+id/imageView2"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    app:srcCompat="@mipmap/etapa_1"
    android:layout_alignParentTop="true"
    android:layout_alignStart="@+id/imgbtn2"
    android:layout_marginTop="21dp"
    android:layout_alignLeft="@+id/imgbtn2" />

<ImageView
    android:id="@+id/imageView4"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    app:srcCompat="@mipmap/selecione_ion"
    android:layout_below="@+id/imageView2"
    android:layout_alignRight="@+id/imageView2"
    android:layout_alignEnd="@+id/imageView2"
    android:layout_marginTop="75dp" />

<ImageButton
    android:id="@+id/imgbtn1"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@android:color/transparent"
    app:srcCompat="@mipmap/icon_prof"
    android:layout_below="@+id/imageView4"
    android:layout_alignParentLeft="true"
    android:layout_alignParentStart="true"
    android:layout_marginTop="54dp" />

<ImageButton
    android:background="@android:color/transparent"

    android:id="@+id/imgbtn2"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    app:srcCompat="@mipmap/icon_atleta"
    android:layout_marginTop="41dp"
    android:layout_below="@+id/imgbtn1"
    android:layout_centerHorizontal="true" />
</RelativeLayout>

